{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "hours": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "start": {
      "type": "string",
      "required": true
    },
    "price_taxless": {
      "type": "integer",
      "required": true
    },
    "intensity": {
      "type": "string",
      "required": true
    },
    "persons": {
      "type": "string",
      "required": true
    },
    "language_levels": {
      "type": "component",
      "repeatable": true,
      "component": "courses-ui.language-level"
    },
    "books": {
      "type": "component",
      "repeatable": true,
      "component": "courses-ui.book"
    }
  }
}
